{
    "name": "yiisoft/help-system2",
    "description": "Help System integration for the Yii framework",
    "keywords": ["yii2", "helpSystem"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "minimum-stability": "stable",
    "support": {
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "authors": [
        {
            "name": "digitalmesh"
        }
    ],
    "require": {
        "yiisoft/yii2": "*"
    },
"alias" : {

					"@yii/helpSystem"         : "/yiisoft/yii2-helpSystem",
					"@yii/helpSystem/admin"   :  "/yiisoft/yii2-helpSystem/modules/admin",
					"@yii/helpSystem/client"  : "/yiisoft/yii2-helpSystem/modules/client",
					"@yii/helpSystem/dmeditor" : "/yiisoft/yii2-helpSystem/vendor/dmeditor",
			
}
}
